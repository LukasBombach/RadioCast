{
  "manifest_version": 2,
  "name": "RadioCast",
  "description": "Play online radio stations on Chromecast devices.",
  "version": "1.5",
  "icons": { "512": "icons/radiocolor512.png" },
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": ["webview", "contextMenus", "storage", "<all_urls>", { "fileSystem": ["write"] } ]
}